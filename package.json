    "@capacitor/status-bar": "^5.0.0",
    "@ionic-enterprise/cs-demo-weather-widgets-vue": "~3.1.0",
    "@ionic/vue": "~7.0.0-beta.0",
    "@ionic/vue-router": "~7.0.0-beta.0",
    "axios": "~1.4.0",
